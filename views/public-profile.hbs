

{{> messages }}
{{#if email}}
<h2>{{ firstName }}'s profile</h2>
<img id="output" style="width: 200px; height: 200px; border-radius: 50%; margin: 30px 0;" src="/uploads/{{img}}" alt="person" />
<table class="table">
  <tr>
    <td>First Name:</td>
    <td>{{ firstName }}</td>
  </tr>
  <tr>
    <td>Last Name:</td>
    <td>{{ lastName }}</td>
  </tr>
  <tr>
    <td>Email:</td>
    <td>{{ email }}</td>
  </tr>
</table>
{{/if}}